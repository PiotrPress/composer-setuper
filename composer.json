{
  "name": "piotrpress/composer-setuper",
  "type": "composer-plugin",
  "license": "MIT",
  "description": "This Composer plugin allows realizing full setup process (also interactively) of your project according to defined steps.",
  "homepage": "https://github.com/PiotrPress/composer-setuper",
  "authors": [ {
    "name": "Piotr Niewiadomski",
    "homepage": "https://piotr.press"
  } ],
  "keywords": [ "setup", "setuper", "composer", "composer-setuper", "composer-script", "composer-plugin", "php" ],
  "require": {
    "php": ">=7.4",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": { "composer/composer": "^2.0" },
  "extra": { "class": "PiotrPress\\Composer\\Setuper\\Plugin" },
  "autoload": {
    "psr-4": { "PiotrPress\\Composer\\Setuper\\": "src/" },
    "classmap": [ "src" ]
  }
}